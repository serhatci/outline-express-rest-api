{
  "name": "outline-express-rest-api",
  "version": "1.0.1",
  "description": "This package reads source code of express rest API files and summarize APi end points and returned values in the terminal",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "test": "jest --watchAll",
    "test-coverage": " jest --collect-coverage --testEnvironment=node --forceExit --maxWorkers=1",
    "lint": "lint-staged",
    "lint-only": "eslint .",
    "lint-all": "pretty-quick && eslint",
    "prepare": "npx husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "keywords": [
    "Express",
    "Rest API"
  ],
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "author": "Serhat Ciftci",
  "license": "SEE LICENCE IN LICENCE",
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.5",
    "@prettier/plugin-pug": "^1.15.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "pretty-quick": "^3.1.0"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jest": "^27.0.6"
  }
}
